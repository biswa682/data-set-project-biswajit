<!DOCTYPE html>
<html>
<head>
	<title>No Of Matches High Chart</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<script src="https://code.highcharts.com/highcharts.js"></script>
	<script src="https://code.highcharts.com/modules/series-label.js"></script>
	<script src="https://code.highcharts.com/modules/exporting.js"></script>
	<script src="https://code.highcharts.com/modules/export-data.js"></script>
	<style>
	#container1 {
		min-width: 310px;
		max-width: 800px;
		height: 400px;
		margin: 0 auto
	}
	</style>
</head>
<body>
	<div class="container1" id="container1">
	</div>
</body>
<script type="text/javascript">
 var listOfMatches = <%-matchNo%>;
 var noOfMatchesList = [];
for(let i in listOfMatches){
	noOfMatchesList.push(listOfMatches[i]);
}
Highcharts.chart('container1', {

    title: {
        text: 'Plot the number of matches played per year of all the years in IPL.'
    },

    subtitle: {
        text: 'IPL Data chart'
    },

    yAxis: {
        title: {
            text: 'Number of Matches Per Year'
        }
    },
    legend: {
        layout: 'vertical',
        align: 'right',
        verticalAlign: 'middle'
    },

    plotOptions: {
        series: {
            label: {
                connectorAllowed: false
            },
            pointStart: 2008
        }
    },

    series: [{
        name: 'Installation',
        data: noOfMatchesList
    }],

    responsive: {
        rules: [{
            condition: {
                maxWidth: 500
            },
            chartOptions: {
                legend: {
                    layout: 'horizontal',
                    align: 'center',
                    verticalAlign: 'bottom'
                }
            }
        }]
    }
}); 
</script>
</html>